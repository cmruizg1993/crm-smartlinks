{% extends 'base.html.twig' %}

{% block title %}Configuracion index{% endblock %}

{% block body %}
    <h1>Configuracion index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Obligado</th>
                <th>RazonSocial</th>
                <th>NombreComercial</th>
                <th>Ruc</th>
                <th>Email</th>
                <th>Telefono</th>
                <th>Direccion</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for configuracion in configuracions %}
            <tr>
                <td>{{ configuracion.id }}</td>
                <td>{{ configuracion.obligadoContabilidad }}</td>
                <td>{{ configuracion.razonSocial }}</td>
                <td>{{ configuracion.nombreComercial }}</td>
                <td>{{ configuracion.ruc }}</td>
                <td>{{ configuracion.email }}</td>
                <td>{{ configuracion.telefono }}</td>
                <td>{{ configuracion.direccion }}</td>
                <td>
                    <a href="{{ path('app_configuracion_show', {'id': configuracion.id}) }}">show</a>
                    <a href="{{ path('app_configuracion_edit', {'id': configuracion.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_configuracion_new') }}">Create new</a>
{% endblock %}
