{% for servicio in servicios %}
<tr>
    <td>
        <input type="radio" name="servicio_id" value="{{ servicio.id }}"
               data-servicio="{{  servicio|json_encode() }}">
    </td>
    <td>{{ servicio.codigo }}</td>
    <td>{{ servicio.nombre }}</td>
    <td>{{ servicio.precio }}</td>
</tr>
{% endfor %}