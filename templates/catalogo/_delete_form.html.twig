<form method="post" action="{{ path('catalogo_delete', {'id': catalogo.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ catalogo.id) }}">
    <button class="btn">Delete</button>
</form>
